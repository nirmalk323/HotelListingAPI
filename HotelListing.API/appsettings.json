{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
      "WriteTo": [
        {
          "Name": "Console"
        },
        {
          "Name": "File",
          "Args": { "path": "./Logs/log.txt" },
          "rollingInterval" :  "Day"
        }
    
    ],
    "Properties": {
      "Application": "Sample"
    }
  },
  "AllowedHosts": "*"
}
